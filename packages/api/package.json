{
  "name": "@mpnpm/api",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "API backend for my app to consume",
  "main": "dist/index.mjs",
  "scripts": {
    "build:prod": "NODE_ENV=production node esbuild.mjs",
    "build:dev": "NODE_ENV=development node esbuild.mjs",
    "dev": "miniflare ./dist/index.mjs --watch --debug --port 8000",
    "format": "prettier --write  '*.{json,js}' 'src/**/*.{js,ts}' 'test/**/*.{js,ts}'",
    "lint": "eslint --max-warnings=0 src && prettier --check '*.{json,js}' 'src/**/*.{js,ts}' 'test/**/*.{js,ts}'",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --config jestconfig.json --verbose"
  },
  "author": "Joseph Curtis",
  "license": "MIT",
  "eslintConfig": {
    "root": true,
    "extends": [
      "typescript",
      "prettier"
    ]
  },
  "dependencies": {
    "faunadb": "^4.5.2",
    "stripe": "^8.186.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.0.0",
    "@types/jest": "^26.0.23",
    "@types/node": "^17.0.16",
    "@types/service-worker-mock": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "dotenv": "^16.0.0",
    "esbuild": "^0.14.23",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-typescript": "^3.0.0",
    "jest": "^27.0.1",
    "@esbuild-plugins/node-globals-polyfill": "^0.1.1",
    "@esbuild-plugins/node-modules-polyfill": "^0.1.4",
    "jest-environment-miniflare": "^2.3.0",
    "miniflare": "^2.3.0",
    "prettier": "^2.3.0",
    "ts-jest": "^27.0.1",
    "typescript": "^4.3.2"
  },
  "peerDependencies": {
    "@types/jest": "^27.0.1"
  }
}
